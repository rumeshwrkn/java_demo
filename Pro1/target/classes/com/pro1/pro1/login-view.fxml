<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="331.0" prefWidth="458.0" styleClass="bg-blue" stylesheets="@Style.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.controllers.LoginController">
   <children>
      <Label layoutX="163.0" layoutY="17.0" text="Hello Portal" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <Label fx:id="LblMessage" layoutX="66.0" layoutY="55.0" prefHeight="20.0" prefWidth="323.0" styleClass="login-error" />
      <Label layoutX="66.0" layoutY="80.0" prefHeight="19.0" prefWidth="323.0" text="Username">
         <styleClass>
            <String fx:value="label-white" />
            <String fx:value="font-size-13" />
         </styleClass>
      </Label>
      <TextField fx:id="TxtEmail" layoutX="66.0" layoutY="99.0" prefHeight="27.0" prefWidth="323.0" promptText="Enter username" styleClass="login-text-field" />
      <Label layoutX="66.0" layoutY="137.0" prefHeight="19.0" prefWidth="323.0" text="Password">
         <styleClass>
            <String fx:value="label-white" />
            <String fx:value="font-size-13" />
         </styleClass>
      </Label>
      <PasswordField fx:id="TxtPass" layoutX="66.0" layoutY="156.0" prefHeight="27.0" prefWidth="323.0" promptText="Enter password" styleClass="login-text-field" />
      <Button fx:id="BtnLogin" layoutX="66.0" layoutY="205.0" mnemonicParsing="false" onAction="#onClickLogin" prefHeight="33.0" prefWidth="323.0" styleClass="login-button" text="Login" />
      <Label layoutX="164.0" layoutY="269.0" styleClass="label-white" text="Forgot password? Reset" textAlignment="CENTER" />
   </children>
</AnchorPane>
